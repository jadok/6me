{
  "name": "markdown-utils",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "clean": "rimraf dist/*",
    "test": "nyc _mocha --require ts-node/register --recursive src/**/*.spec.* test/**/*.spec.*",
    "test:dev": "_mocha --require ts-node/register --recursive test/**/*.spec.*",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint:all": "npm run lint && npm run tscCheck",
    "lint:fx": "eslint src/**/*.ts --fix",
    "lint": "eslint src/**/*.ts",
    "start": "tsc && node dist/index.js",
    "build": "tsc",
    "tscCheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jadok/6me.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jadok/6me/issues"
  },
  "homepage": "https://github.com/jadok/6me#readme",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "chai": "^4.3.4",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.61",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:all"
    }
  },
  "dependencies": {}
}
